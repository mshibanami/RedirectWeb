{
  "bundleID": "io.github.mshibanami.RedirectWebForSafari",
  "kind": "RedirectList",
  "redirects": [
    {
      "appURL": "file:\/\/\/Applications\/Google%20Chrome.app",
      "comments": "Use this rule to make Google Meet links in Safari open in Chrome automatically.",
      "destinationURLPattern": "$0",
      "exampleURLs": [
        "https:\/\/meet.google.com\/xxx-yyyy-zzz"
      ],
      "excludeURLPatterns": [],
      "kind": "Redirect",
      "sourceURLPattern": {
        "type": "regularExpression",
        "value": "https:\/\/meet.google.com\/[a-z]*-[a-z]*-[a-z]*"
      },
      "title": "Google Meet: Open in Chrome"
    }
  ]
}
